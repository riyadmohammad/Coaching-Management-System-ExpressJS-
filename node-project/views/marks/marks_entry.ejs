                                    <input type="number" class="form-control" id="courseId" name="course" placeholder="enter courseId">
                                </div>

                                <div class="form-group">
                                    <h5> UserId </h5>
                                    <input type="number" class="form-control" id="userId" name="user" placeholder="enter userId" required="">
                                </div>

                                <div class="form-group">
                                    <h5> Marks</h5>
                                    <input type="number" class="form-control" id="marksId" name="marks" placeholder="enter marks">
                                </div>
                                <button type="submit" class="btn btn-primary">upload</button>
