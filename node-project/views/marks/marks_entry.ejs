                            <form class="form" action="" method="post">

                                <div class="form-group">
                                <h5>Exam Type</h5>
                                <select class="form-control" name="examType" id="examType">
                                    <option> Quiz </option>
                                    <option> Written </option>
                                    <option> Objective </option>
                                </select>
                                </div>


